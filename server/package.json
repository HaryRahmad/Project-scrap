{
    "name": "antam-monitor-server",
    "version": "1.0.0",
    "description": "Backend API untuk SaaS Gold Stock Monitor",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "sequelize": "^6.35.2"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "sequelize-cli": "^6.6.2"
    }
}